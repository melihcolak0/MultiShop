@model UserDetailViewModel

@{
    Layout = null;
}

<link href="/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

<div class="container">
    <br /> <br />
    <h3>Kullanıcı Profil Bilgisi</h3>
    <br />
    <div class="alert alert-info">Kullanıcı Id: @Model.Id</div>
    <div class="alert alert-warning">Kullanıcı Adı: @Model.Username</div>
    <div class="alert alert-info">Adı: @Model.Name</div>
    <div class="alert alert-warning">Soyadı: @Model.Surname</div>
    <div class="alert alert-info">E-Posta Adresi: @Model.Email</div>
</div>
